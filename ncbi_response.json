{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["request_id", "status"],
  "properties": {
    "request_id": {
      "type": "string",
      "format": "uuid"
    },
    "status": {
      "type": "string",
      "enum": ["ok", "error"]
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": true
      }
    },
    "pagination": {
      "type": "object",
      "properties": {
        "retstart": {
          "type": "integer",
          "minimum": 0
        },
        "retmax": {
          "type": "integer",
          "minimum": 1
        },
        "count": {
          "type": "integer",
          "minimum": 0
        }
      }
    },
    "provenance": {
      "type": "object",
      "properties": {
        "queried_at": {
          "type": "string",
          "format": "date-time"
        },
        "api_url": {
          "type": "string"
        },
        "webenv": {
          "type": "string"
        },
        "query_key": {
          "type": "string"
        }
      }
    },
    "error": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer"
        },
        "message": {
          "type": "string"
        }
      }
    }
  }
}